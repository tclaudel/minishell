echo 'ls'
printf "\33[2K\r\n###############\ntesting : %s\n###############\n\n" 'ls > ble'
ls > ble
printf "\33[2K\r\n###############\ntesting : %s\n###############\n\n" 'cat < ble'
cat < ble
printf "\33[2K\r\n###############\ntesting : %s\n###############\n\n" 'echo $USER'
echo $USER
printf "\33[2K\r\n###############\ntesting : %s\n###############\n\n" ''ls > ble''
'ls > ble'
printf "\33[2K\r\n###############\ntesting : %s\n###############\n\n" '"ls '>' ble"'
"ls '>' ble"
printf "\33[2K\r\n###############\ntesting : %s\n###############\n\n" ''echo 'ls'''
'echo 'ls''
printf "\33[2K\r\n###############\ntesting : %s\n###############\n\n" ''echo 'ls'' pouet'
'echo 'ls'' pouet
printf "\33[2K\r\n###############\ntesting : %s\n###############\n\n" ''echo 'ls'' pouet'
'echo 'ls'' pouet
printf "\33[2K\r\n###############\ntesting : %s\n###############\n\n" ''ech'o' 'pouet'''
